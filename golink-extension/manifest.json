{
  "name": "Firework Go Link",
  "description": "Navigate to shortcut path with go/path",
  "version": "1.0",
  "manifest_version": 3,
  "omnibox": { "keyword": "go" },
  "permissions": ["declarativeNetRequest"],
  "host_permissions": ["http://go/*"],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  },
  "background": {
    "service_worker": "background.js"
  }
}
